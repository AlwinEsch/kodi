<?xml version="1.0" encoding="UTF-8"?>
<addon
  id="adsp.template"
  version="0.0.1"
  name="Audio DSP Template"
  provider-name="Team XBMC">
  <requires>
    <c-pluff version="0.1"/>
    <import addon="xbmc.adsp" version="0.0.1"/>
  </requires>
  <extension
    point="xbmc.adspaddon"
    library_linux="XBMC_Template.adsp"
    library_osx="XBMC_Template.adsp"
    library_wingl="XBMC_adsp_template_win32.dll"
    library_windx="XBMC_adsp_template_win32.dll"
    library_android="libXBMC_adsp_template.so" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Template for a Audio DSP Addon</summary>
    <description lang="en">Loopback, do no Signal Processing</description>
    <disclaimer lang="en">Just contains stubs</disclaimer>
    <platform>@OS@</platform>
  </extension>
</addon>
